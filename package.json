{
  "author"      : "Synapse Studios",
  "name"        : "frontend-template",
  "version"     : "0.0.0",
  "description" : "A quickstart frontend template that expects to be paired with a backend based off of synapsestudios/api-template",
  "license"     : "MIT",
  "repository"  : {
    "type" : "git",
    "url"  : "https://github.com/synapsestudios/frontend-template.git"
  },
  "scripts" : {
        "start"   : "rm -rf ./build && webpack && node webpack",
        "webpack" : "rm -rf ./build && webpack",
        "dist"    : "rm -rf ./build && APP_ENV=production webpack -p && zip -r build-`date +%s` application build node_modules server server.js",
        "test"    : "cd __tests__ && rm -rf ./build && webpack && node webpack",
        "karma"   : "./node_modules/karma/bin/karma start karma.conf.js",
        "clean"   : "rm -rf ./build"
  },
  "devDependencies": {
    "chai"                     : "1.9.1",
    "es5-shim"                 : "4.0.5",
    "karma"                    : "0.12.21",
    "karma-chrome-launcher"    : "0.1.4",
    "karma-firefox-launcher"   : "0.1.3",
    "karma-mocha"              : "0.1.7",
    "karma-phantomjs-launcher" : "0.1.4",
    "karma-sinon-chai"         : "0.3.0",
    "karma-sourcemap-loader"   : "0.3.3",
    "karma-webpack"            : "1.5.0",
    "mocha"                    : "1.21.4",
    "react-tools"              : "0.11.1",
    "sinon"                    : "1.10.3",
    "sinon-chai"               : "2.5.0",

    "autoprefixer-loader"         : "1.1.0",
    "css-loader"                  : "0.9.1",
    "6to5-loader"                 : "3.0.0",
    "extract-text-webpack-plugin" : "0.3.8",
    "file-loader"                 : "0.8.1",
    "html-webpack-plugin"         : "1.1.0",
    "image-webpack-loader"        : "1.2.0",
    "json-loader"                 : "0.5.1",
    "jshint-loader"               : "0.8.1",
    "jsx-loader"                  : "0.11.2",
    "jsxhint-loader"              : "0.2.0",
    "null-loader"                 : "0.1.1",
    "raw-loader"                  : "0.5.1",
    "react-hot-loader"            : "1.1.4",
    "sass-loader"                 : "0.4.1",
    "style-loader"                : "0.8.3",
    "url-loader"                  : "0.5.5",
    "webpack"                     : "1.5.3",
    "webpack-dev-server"          : "1.7.0",
    "webpack-error-notification"  : "0.1.4"
  },
  "dependencies": {
    "fluxxor"         : "1.5.1",
    "react"           : "0.12.2",
    "react-router"    : "0.12.0",
    "react-intl"      : "1.1.0",
    "intl"            : "0.1.4",
    "request"         : "2.53.0",
    "store"           : "1.3.16",
    "synapse-common"  : "1.0.0",
    "synfrastructure" : "git://github.com/synapsestudios/synfrastructure.git#master",
    "underscore"      : "1.6.0"
  }
}
